    "i18n-iso-countries": "^5.4.0",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.25.3",
    "mousetrap": "^1.6.5",
    "numeral": "^2.0.6",
    "path-to-regexp": "^6.1.0",
    "qs": "^6.9.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.1.2",
