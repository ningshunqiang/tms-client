    "react-router-query-hook": "^1.1.0",
    "react-use": "^15.1.0",
    "remove": "^0.1.5",
    "slash": "^3.0.0",
    "styled-components": "^5.1.0",
    "text-hex": "^1.0.0",
    "umi": "^3.2.1",
    "url-parse": "^1.4.7"
