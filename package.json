    "@graphql-codegen/typescript-react-apollo": "^1.14.0",
    "@storybook/addon-actions": "^5.3.15",
    "@storybook/addon-info": "^5.3.15",
    "@storybook/addon-links": "^5.3.15",
    "@storybook/addons": "^5.3.15",
